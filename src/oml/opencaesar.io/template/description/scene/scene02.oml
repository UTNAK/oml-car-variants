	instance ads-test-case01 : adsdomain:AutomatedDrivingSystemDomain [
		adsdomain:hasAV sv
		adsdomain:hasJASPARODD odd-2
		base:hasDescription "Table 60. Perform Lane Change Test Scenarios Complex Positive 15 PLC_CP_15"
	]


	instance odd-2 : adsdomain:JASPARODD[
		adsdomain:hasEC lowRain
		adsdomain:hasEC headlights
		adsdomain:hasPI intersections
		adsdomain:hasPI rails
		adsdomain:hasEC day
		adsdomain:hasTP pov-1
		adsdomain:MinimumRelativeDistanceX "0.0"^^xsd:float // Example value for minimum relative distance in X
		adsdomain:MinimumRelativeDistanceY "3.0"^^xsd:float // Example value for minimum relative distance in Y
	]

	instance sv : adsdomain:AutomatedVehicle[
		adsdomain:hasVehicleType sv.type
		adsdomain:hasVelocity sv.velocity
		adsdomain:hasPos-X sv.pos.x
		adsdomain:hasPos-Y sv.pos.y
		adsdomain:hasPos-Z sv.pos.z
		adsdomain:hasAcceleration sv.acceleration
		adsdomain:hasSteering sv.steering
		adsdomain:hasOrientation sv.orientation
	]

	instance sv.type : adsdomain:VehicleType[
		adsdomain:hasString "Sedan"
	]
	instance sv.pos.x : adsdomain:Pos-X[
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]
	instance sv.pos.y : adsdomain:Pos-Y [
		adsdomain:hasFloatNumber "-3.6"^^xsd:float
	]
	instance sv.pos.z : adsdomain:Pos-Z [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]
	instance sv.velocity : adsdomain:Velocity[
		adsdomain:hasFloatNumber "6.6667"^^xsd:float
	]
	instance sv.acceleration : adsdomain:Acceleration[
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]
	instance sv.steering : adsdomain:Steering[
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]
	instance sv.orientation : adsdomain:Orientation[
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]



	instance lowRain : weather:LowRain[
		EnverionmentalCondition:weatherPattern "SoftRainNoon"
		// EnverionmentalCondition:cloudiness "20.0"^^xsd:float
		// EnverionmentalCondition:dust_storm "30.0"^^xsd:float
