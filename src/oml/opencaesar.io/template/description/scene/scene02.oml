	instance intersections : road:Intersections
	instance rails : roadFurniture:Rails
	instance day : illumination:Day


	// Rear bumper 8 m (25 ft) in front of SV front bumper
	instance pov-1 : roaduser:ManualMovingVehicles [
		adsdomain:hasVehicleType pv1.type
		adsdomain:hasPos-X pv1.pos.x
		adsdomain:hasPos-Y pv1.pos.y
		adsdomain:hasPos-Z pv1.pos.z
		adsdomain:hasVelocity pv1.velocity
		adsdomain:hasAcceleration pv1.acceleration
		adsdomain:hasSteering pv1.steering
		adsdomain:hasOrientation pv1.orientation
	]

	instance pv1.type : adsdomain:VehicleType [
		adsdomain:hasString "Car"
	]

	instance pv1.pos.x : adsdomain:Pos-X [
		adsdomain:hasFloatNumber "8.0"^^xsd:float
	]

	instance pv1.pos.y : adsdomain:Pos-Y [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]

	instance pv1.pos.z : adsdomain:Pos-Z [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	] // instance environmentalCondition : adsdomain:EnverionmentalCondition

	instance pv1.velocity : adsdomain:Velocity [
		adsdomain:hasFloatNumber "6.6667"^^xsd:float
	] // instance environmentalCondition : adsdomain:EnverionmentalCondition

	instance pv1.acceleration : adsdomain:Acceleration [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]

	instance pv1.steering : adsdomain:Steering [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]

	instance pv1.orientation : adsdomain:Orientation [
		adsdomain:hasFloatNumber "0.0"^^xsd:float
	]



